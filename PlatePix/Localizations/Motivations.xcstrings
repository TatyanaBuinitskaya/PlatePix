        }
      }
    },
    "Motivation20" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Don't do what's easy, do what will lead you to a beautiful body"
